{
  "name": "entrypoint-analysis",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Self-analysis tools for identifying entry points in all packages",
  "scripts": {
    "analyze": "npx tsx src/self_analysis/detect_entrypoints.ts",
    "profile": "clinic doctor -- node --import tsx src/self_analysis/detect_entrypoints.ts",
    "profile:flame": "clinic flame -- node --import tsx src/self_analysis/detect_entrypoints.ts",
    "profile:simple": "NODE_ENV=production node --cpu-prof --cpu-prof-interval=100 --import tsx src/self_analysis/detect_entrypoints.ts",
    "profile:detailed": "ARIADNE_PROFILE=1 npx tsx src/self_analysis/detect_entrypoints.ts --stdout"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "*",
    "@ariadnejs/core": "file:./packages/core",
    "@ariadnejs/types": "file:./packages/types"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "clinic": "^13.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  }
}
